// Code to implement variables

// declaring a class

public class hello {

   public static void main(String[] args){
       System.out.println("Hello World!!");     // print statement

       int var_1 = 556; // declaring  variable
       int var_2 = 710;
       int var_3 = 100;

       int output = var_1 + var_2 +var_3;
       System.out.println("output = ");
       System.out.println(output);

       int sub = 1000 - output;
       System.out.println(sub);
   }
}
